                        <efbc:ChangedSectionIdentifier>RES-0001</efbc:ChangedSectionIdentifier>
                     </efac:ChangedSection>
                  </efac:Change>
                  <efac:ChangeReason>
                     <cbc:ReasonCode listName="change-corrig-justification">update-add</cbc:ReasonCode>
                     <efbc:ReasonDescription languageID="DEU">Auftragserweiterung nach § 132 Abs. 2 S. 1 Nr. 2 und Abs. 2 Nr. 3 GWB </efbc:ReasonDescription>
                  </efac:ChangeReason>
               </efac:Changes>
